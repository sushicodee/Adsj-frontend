og/admin', {\n      Bucket: 'blog-images-1',\n      key: blog.image,\n      id: blog.id,\n    });\n    dispatch({ type: DELETE_BLOG, payload: data });\n  } catch (err) {\n    console.log(err);\n  }\n};\n\nexport const setCurrentPage = (val) => (dispatch) => {\n  dispatch({\n    type: SET_CURRENT_PAGE,\n    payload: val,\n  });\n};\n\n//accepts <= 100\nexport const setPerPage = (val) => (dispatch) => {\n  dispatch({\n    type: SET_PER_PAGE,\n    payload: val,\n  });\n};\n","/home/sushicodee/Documents/side-projects/travel-app/Adsj-frontend/src/components/pages/blogs/SingleBlog.tsx",["280","281"],"import React, { useContext } from 'react';\nimport { IBlog } from 'interfaces/blog';\nimport { AuthContext } from 'context/authContext';\nimport { Container, Grid, makeStyles, Typography } from '@material-ui/core';\nimport moment from 'moment';\ninterface IProps {\n  blog: IBlog;\n}\n\nconst useStyles = makeStyles((theme) => ({\n  root: {\n    marginTop: theme.spacing(20),\n  },\n  imageContainer: {\n    aspectRatio: '16:4',\n  },\n  image: {\n    width: '100%',\n  },\n}));\n\nconst SingleBlog: React.FC<IProps> = (props) => {\n  const classes = useStyles();\n  const { user } = useContext(AuthContext);\n  const { title, image, description, createdAt } = props?.blog;\n  return (\n    <Container className={`${classes.root} card`}>\n      <Grid item>\n        {image ? (\n          <Grid className={classes.imageContainer}>\n            <img className={classes.image} src={image} />\n          </Grid>\n        ) : null}\n        <Typography variant='h2'>{title && title} </Typography>\n        <Typography variant='h5'>\n          {' '}\n          {moment(createdAt).fromNow().toUpperCase()}{' '}\n        </Typography>\n        <Typography variant='h4'>{description && description}</Typography>\n      </Grid>\n    </Container>\n  );\n};\n\nexport default SingleBlog;\n","/home/sushicodee/Documents/side-projects/travel-app/Adsj-frontend/src/components/pages/blogs/components/BlogFormCard.t
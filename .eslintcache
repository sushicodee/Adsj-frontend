 dispatch(fetchFeaturedBlogs());\n      // dispatch(fetchMainFeaturedBlog());\n    }\n  }, []);\n\n  useEffect(() => {\n    console.log(cachedPages.includes(currentPage));\n    if (!cachedPages.includes(currentPage)) {\n      console.log('fetching blogs');\n      dispatch(fetchBlogs(currentPage, perPage));\n    }\n  }, [currentPage, perPage]);\n\n  const handlePageChange = (\n    event: React.ChangeEvent<unknown>,\n    value: number\n  ) => {\n    dispatch(setCurrentPage(value));\n  };\n  return (\n    <>\n      <CssBaseline />\n      <Container maxWidth='lg'>\n        <main>\n          <MainFeaturedPost post={mainFeaturedBlog} />\n          {user && user.role === 'admin' && <BlogFormCard />}\n          <Grid container spacing={4}>\n            {!isFeaturedprogress ? (\n              featuredBlogs.map((post) => (\n                <FeaturedPosts key={post.id + 'featured'} post={post} />\n              ))\n            ) : (\n              <CircularProgress />\n            )}\n          </Grid>\n          <Grid container spacing={5} className={classes.mainGrid}>\n            <Grid item xs={12} md={8}>\n              {!isBlogsProgress ? (\n                <MainSection\n                  title='All Posts'\n                  blogs={blogs}\n                  currentPage={currentPage}\n                  perPage={perPage}\n                />\n              ) : (\n                <CircularProgress />\n              )}\n              <CustomPagination\n                count={Math.ceil(blogsCount / perPage)}\n                handleChange={handlePageChange}\n                currentPage={currentPage}\n              />\n            </Grid>\n            <Sidebar\n              title={sidebar.title}\n              description={sidebar.description}\n              archives={sidebar.archives}\n              social={sidebar.social}\n            />\n          </Grid>\n          <Grid container spacing={4}>\n            <Contact history={props.history} />\n          </Grid>\n        </mai